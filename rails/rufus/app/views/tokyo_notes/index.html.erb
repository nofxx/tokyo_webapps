<h1>Listing tokyo_notes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th>Rate</th>
  </tr>

<% @tokyo_notes.each do |tokyo_note| %>
  <tr>
    <td><%=h tokyo_note.title %></td>
    <td><%=h tokyo_note.body %></td>
    <td><%=h tokyo_note.rate %></td>
    <td><%= link_to 'Show', tokyo_note %></td>
    <td><%= link_to 'Edit', edit_tokyo_note_path(tokyo_note) %></td>
    <td><%= link_to 'Destroy', tokyo_note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New note', new_tokyo_note_path %>
